{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@scena/react-guides": "^0.14.0",
    "@umijs/preset-react": "1.x",
    "gesto": "^1.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "umi": "^3.4.7",
    "react-sortable-hoc": "^1.11.0",
    "react-viewer": "^3.2.2",
    "copy-text-to-clipboard": "^2.2.0",
    "nanoid": "^2.1.11",
    "rc-color-picker": "^1.2.6",
    "clone": "^2.1.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/test": "^3.4.7",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0",
    "form-render": "^0.10.5",
    "classnames": "2.2.6"
  }
}
